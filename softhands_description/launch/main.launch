<?xml version="1.0" encoding="UTF-8"?>
<launch>

	<!-- start gazebo server with empty world -->
	<include file="$(find gazebo_ros)/launch/empty_world.launch">
		<arg name="world_name" value="$(find softhands_description)/worlds/empty.world"/>
		<arg name="paused" value="true"/>
    	<arg name="use_sim_time" value="true"/>
    	<arg name="gui" value="true"/>
    	<arg name="headless" value="false"/>
    	<arg name="verbose" value="false"/>
    	<arg name="debug" value="false"/>
	</include>	

	<!-- start gazebo server with empty world -->
	<!-- <include file="$(find softhands_description)/launch/softhands_gazebo.launch">
		<arg name="sh_version" value="v1_2_research"/>
		<arg name="x" value="0"/>
		<arg name="y" value="0"/>
		<arg name="z" value="0"/>
		<arg name="friction" value="1e-2"/>
  		<arg name="suffix" value="0"/>
  		<arg name="virtual_friction" value="1e-2"/>
	</include>	 -->
	
	<include file="$(find softhands_description)/launch/softhands_gazebo.launch">
		<arg name="sh_version" value="v1_wide"/>
		<arg name="x" value="0.3"/>
		<arg name="y" value="0"/>
		<arg name="z" value="0"/>
  		<arg name="yaw" value="1.57"/>
  		<arg name="friction" value="1e-2"/>
  		<arg name="virtual_friction" value="1e-2"/>
  		<arg name="suffix" value="1"/>
	</include>	
</launch>




